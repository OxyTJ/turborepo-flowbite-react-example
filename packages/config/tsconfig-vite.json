{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "config/tsconfig-base.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "sourceMap": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "types": ["vite/client"]
  },
  "exclude": ["node_modules", "dist"]
}
